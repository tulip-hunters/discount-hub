
<div class="container">
<div class="row">

{{#each shops}}
<div class="col-12 col-md-6 col-lg-4 col-xl-3 single-shop">
    <div class="card">
  <img src="/images/{{this.image}}" class="card-img-top" alt="product image">
  <div class="card-body">
    {{!-- <p class="card-title fw-bolder text-center">{{this.name}} </p> --}}
      <p class="text-center fw-semibold">Visit official page: <a class="fw-semibold"  href="{{this.link}}">{{this.name}}</a></p>
  </div>
</div>
</div>
{{/each}}
</div>
</div>
